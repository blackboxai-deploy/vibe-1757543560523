{
  "name": "skvchatgb",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://skvchatgb.vercel.app",
    "NEXT_PUBLIC_APP_NAME": "SKV.ChatGB"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options", 
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/chatgb",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/skv",
      "destination": "/", 
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/skv-chatgb",
      "destination": "/"
    }
  ]
}